# Resend API Key for email notifications
RESEND_API_KEY=your_resend_api_key_here

# Go High Level API Configuration
# Get these from your GHL app settings
GHL_CLIENT_ID=your_ghl_client_id_here
GHL_CLIENT_SECRET=your_ghl_client_secret_here
GHL_LOCATION_ID=your_ghl_location_id_here
GHL_ACCESS_TOKEN=your_ghl_access_token_here

# Optional: GHL API Version (defaults to 2021-07-28)
GHL_API_VERSION=2021-07-28

# Optional: Enable/disable GHL integration
GHL_ENABLED=true